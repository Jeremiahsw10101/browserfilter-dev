{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/download",
      "dest": "/download.html"
    },
    {
      "src": "/update/(.*)",
      "dest": "/update/$1.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}